{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Imwebs-Viewer",
  "version": "0.2.0",
  "identifier": "ca.eccc.imwebs-viewer",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run vue-dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "eccc-imwebs-viewer",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "externalBin": [
      "../backend/apppy/apppy"
    ],
    "linux": {
      "appimage": {
        "files": {
          "/usr/_up_/backend/apppy": "../_up_/backend/apppy/**/*"
        }
      }
    },
    "icon": [
      "icons/icon.ico",
        "icons/icon_128x128.ico",
        "icons/icon_256x256.ico",
        "icons/icon_512x512.ico",
        "icons/icon_128x128.png",
        "icons/icon_256x256.png",
        "icons/icon_512x512.png",
        "icons/icon-1.png",
        "icons/icon-2.png",
        "icons/icon-3.png",
        "icons/icon-4.png",
        "icons/icon-5.png",
        "icons/icon-6.png",
        "icons/icon-7.png",
        "icons/icon-8.png",
        "icons/icon-9.png"
    ],
    "resources": [
      "../backend/apppy/**/*",
      "../backend/requirements.txt"
    ]
  }
}
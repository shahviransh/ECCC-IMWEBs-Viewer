{
  "name": "eccc-imwebs-viewer",
  "private": true,
  "description": "A brief description of your app.",
  "author": "Viransh Shah",
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "prestart": "node convert-package.js",
    "dev": "concurrently \"npm run vue-dev\" \"npm run python-dev\"",
    "python-dev": "conda activate venv && python -Xfrozen_modules=off -m debugpy --listen 5678 --wait-for-client backend\\apppy.py",
    "vue-dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "npx electron . && vite",
    "electron:build": "vite build && electron-builder",
    "tauri": "node src-tauri/main.js && tauri",
    "tauri:build": "node src-tauri/main.js && tauri build && node src-tauri/move.js"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.2.0",
    "@tauri-apps/cli": "^2.2.2",
    "@tauri-apps/plugin-shell": "^2.2.0",
    "axios": "^1.7.9",
    "dompurify": "^3.2.3",
    "echarts": "^5.6.0",
    "json5": "^2.2.3",
    "vue": "^3.3.4",
    "vue-echarts": "^7.0.3",
    "vue-multiselect": "^3.1.0",
    "vue-router": "^4.5.0",
    "vue-tree-list": "^1.5.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "electron": "^32.2.8",
    "electron-builder": "^25.1.8",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "vite": "^5.4.11"
  }
}
